local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionKey"})


-- functionss
function MakeScriptHub()
    wait(0.5)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/jmxploits/meme-hubb/main/meme%20hubbb"))()
end

function CorrectKeyNotif()
    OrionLib:MakeNotification({
	      Name = "Key Notification",
	      Content = "Correct Key!",
	      Image = "rbxassetid://4483345998",
	      Time = 5
    })
end

function IncorrectKeyNotif()
    OrionLib:MakeNotification({
	      Name = "Key Notification",
	      Content = "Incorrect Key!",
	      Image = "rbxassetid://4483345998",
	      Time = 5
    })
end

-- Values
getgenv().Key = "silenced"
getgenv().KeyInput = "string"


-- The Windows
OrionLib:MakeNotification({
	Name = "Hello!",
	Content = "You are logged in as "..Player.Name.."!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Input Key",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      if getgenv().KeyInput == getgenv().Key then
      MakeScriptHub()
      CorrectKeyNotif()
      else
          IncorrectKeyNotif()
      end         
  end    
})

